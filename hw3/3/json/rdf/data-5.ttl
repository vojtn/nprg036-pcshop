ex:Brand1  rdf:type   schema:Brand;
        schema:email  "support@logitech.com";
        schema:name   "Logitech"@en .

ex:Brand2  rdf:type   schema:Brand;
        schema:email  "support@amd.com";
        schema:name   "AMD"@en .

